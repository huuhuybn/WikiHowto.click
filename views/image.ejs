</script>
<script type="text/javascript">
    function showErrorUI(msg) {
        const btnRender = document.querySelector(".btn-render");
        document.querySelector(".alert-render").innerHTML = msg;
        btnRender.style.opacity = "1";
        btnRender.disabled = !1;
        document.querySelector(".render-wrapper").classList.remove("show")
    }

    function showRenderCompletedUI(download_url) {
        const btnDownRender = document.querySelector(".download-render");
        btnDownRender.classList.add("show");
        btnDownRender.href = download_url
    }

    function updateProgressUI(progress) {
        progress > 0 && 100 !== progress && (document.querySelector(".render-label").innerHTML = "Rendering..."), 100 === progress && (document.querySelector(".render-label").innerHTML = "Render Completed"), document.querySelector(".progress-bar").style.width = progress + "%", document.querySelector(".progress-bar").innerHTML = progress + "%"
    }

    function startRender(e) {
        const btnRender = document.querySelector(".btn-render");
        btnRender.style.opacity = "0.5";
        btnRender.disabled = !0;
        document.querySelector(".alert-render").innerHTML = "";
        document.querySelector(".render-wrapper").classList.add("show")
    }
</script>
