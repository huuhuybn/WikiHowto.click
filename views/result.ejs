    gtag('event', 'download_success', {
      event_category: 'download',
      event_label: analyticsData.platform,
      custom_parameter_1: analyticsData.platform,
      custom_parameter_2: 'success',
      custom_parameter_3: analyticsData.hasVideo ? 'video' : 'audio',
      value: analyticsData.dataCount
    });

    // Track conversion
    gtag('event', 'conversion', {
      event_category: 'conversion',
      event_label: analyticsData.platform,
      custom_parameter_1: analyticsData.platform,
      custom_parameter_2: 'completed',
      value: 1
    });
  }

  // Track download button clicks
  document.addEventListener('DOMContentLoaded', function() {
    const downloadButtons = document.querySelectorAll('.button[href*="download"], .download-link');
    downloadButtons.forEach(function(button) {
      button.addEventListener('click', function(e) {
        gtag('event', 'download_click', {
          event_category: 'engagement',
          event_label: analyticsData.platform || 'unknown',
          custom_parameter_1: analyticsData.platform || 'unknown',
          value: 1
        });
      });
    });
  });
</script>
</body>
</html>
